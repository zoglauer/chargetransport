libs ( "libOpenFOAM.so" "libgroovyBC.so" ) ;

application     spacechargeelectrostaticFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         10; //0.02;

deltaT          1; //5e-05;

writeControl    timeStep;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


