    {
        type            symmetryPlane;
    }

    down
    {
        type            symmetryPlane;
    }

    up
    {
        type            zeroGradient;
    }

*/

    front
    {
        type            fixedValue;
        value           uniform 0;
    }

		back
		{
				type 						fixedValue;
				value						uniform 0;

    defaultFaces
    {
        type            empty;
